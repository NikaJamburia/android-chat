<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ge.nika.chat">

    <application
        android:name="ge.nika.chat.ApplicationContainer"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hello_world">
        <activity android:name="ge.nika.chat.mobile.activity.DisplayMessagesActivity"
            android:label="Inbox"
            android:parentActivityName="ge.nika.chat.mobile.activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ge.nika.chat.mobile.activity.MainActivity" />
        </activity>
        <activity android:name="ge.nika.chat.mobile.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>